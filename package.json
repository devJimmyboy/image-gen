{
	"name": "remotion-template-still",
	"version": "1.0.0",
	"description": "A template for designing still images with dynamic data with built-in server for deploying to the cloud.",
	"scripts": {
		"start": "tsc && node dist/server.js",
		"dev": "remotion preview src/index.tsx",
		"render": "remotion still src/index.tsx TwitchPanel out/still.png --props=./props.json",
		"upgrade": "remotion upgrade",
		"server": "ts-node server.tsx",
		"test": "eslint src --ext ts,tsx,js,jsx && tsc"
	},
	"repository": {},
	"license": "UNLICENSED",
	"dependencies": {
		"@iconify/react": "^3.1.4",
		"@remotion/bundler": "^2.6.11",
		"@remotion/cli": "^2.6.11",
		"@remotion/eslint-config": "^2.6.11",
		"@remotion/renderer": "^2.6.11",
		"@types/express": "^4.17.13",
		"@types/express-rate-limit": "^5.1.3",
		"@types/react": "^17.0.40",
		"@types/web": "^0.0.56",
		"dotenv": "^16.0.0",
		"eslint": "^8.11.0",
		"express": "^4.17.3",
		"express-rate-limit": "^6.3.0",
		"p-limit": "^4.0.0",
		"prettier": "2.5.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"remotion": "^2.6.11",
		"ts-node": "^10.7.0",
		"typescript": "4.6.2"
	}
